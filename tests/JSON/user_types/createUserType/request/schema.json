{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "pattern": "^user_types$"
        },
        "attributes": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            },
            "desc": {
              "type": "string"
            }
          }
        }
      },
      "required": [
        "type",
        "attributes"
      ]
    }
  },
  "required": [
    "data"
  ]
}