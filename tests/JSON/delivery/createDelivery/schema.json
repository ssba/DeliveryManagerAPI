{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "pattern": "^delivery$"
        },
        "id": {
          "type": "string",
          "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$"
        },
        "attributes": {
          "type": "object",
          "properties": {
            "count": {
              "type": "integer"
            }
          },
          "required": [
            "count"
          ]
        },
        "relationships": {
          "type": "object",
          "properties": {
            "delivery_routes": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string",
                      "pattern": "^delivery_routes$"
                    },
                    "id": {
                      "type": "string",
                      "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$"
                    }
                  },
                  "required": [
                    "type",
                    "id"
                  ]
                }
              },
              "required": [
                "data"
              ]
            },
            "products": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string",
                      "pattern": "^products$"
                    },
                    "id": {
                      "type": "string",
                      "pattern": "^[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}$"
                    }
                  },
                  "required": [
                    "type",
                    "id"
                  ]
                }
              },
              "required": [
                "data"
              ]
            }
          },
          "required": [
            "delivery_routes",
            "products"
          ]
        }
      },
      "required": [
        "type",
        "attributes",
        "relationships"
      ]
    }
  },
  "required": [
    "data"
  ]
}